def find_smallest(a, b, c):
    if a <= b and a <= c:
        return a
    elif b <= a and b <= c:
        return b
    else:
        return c

# Example usage
num1 = 10
num2 = 25
num3 = 20

smallest = find_smallest(num1, num2, num3)
print(f"The smallest number among {num1}, {num2}, and {num3} is: {smallest}")
